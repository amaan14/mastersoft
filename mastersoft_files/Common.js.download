
var RenderDropDown = function (control, data, id, value) {
    control.empty();
    $.each(data, function () {
        control.append($("<option />").val(this[id]).text(this[value]));
    }); control.prepend($("<option selected='selected' />").val(0).text("Please Select"));
};

